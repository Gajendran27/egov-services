def build(path, ci_image) {
    stage("Build"){
        docker.image("${ci_image}").inside {
            sh "apk update && apk add openssl && wget  https://github.com/pinterest/secor/archive/master.zip -O master.zip &&  unzip master.zip && cd secor-master && mvn package";
        }
    }
 }

 return this;
